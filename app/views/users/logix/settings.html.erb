<% content_for :title, "CircuitVerse - Settings" %>

<div class="container">
  <div class="row row-about">
    <div class="col-xs col-sm col-md col-lg">
      <h1 class="heading-profile">User Settings</h1>
    </div>
  </div>
  <div id="settings-container" class="row profile-background container px-5">
    <p> Loading your data </p>
  </div>
</div>

<script>
    loadUserSettings(JSON.parse('<%= raw current_user.settings.to_json %>'), <%= Rails.env == "development" %>, '<%= form_authenticity_token %>');
</script>
